                                <div>
                                    <apex:outputpanel >
                                        <apex:outputLabel styleClass="top-level-label" value="Preferred Communication Language:" />
                                        <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Preferred_Language__c}" />
                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel rendered="{! LicenseRecordTypeName != 'FCC_Assistant_Change_of_Address'}">
                                        <apex:outputLabel styleClass="top-level-label" value="Professional Qualification Registry Number:" />
                                        <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Prof_Qualification_Reg_Number__c}" />
                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel >
                                        <apex:outputLabel styleClass="top-level-label" value="Can you communicate in English?:" />
                                        <apex:selectradio value="{!licenseTransation.Can_Communicate_in_English__c}" disabled="true">
                                            <apex:selectoptions value="{!items}" />
                                        </apex:selectradio>
                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel rendered="{! LicenseRecordTypeName != 'FCC_Assistant_Change_of_Address'}">
                                        <apex:outputLabel styleClass="top-level-label" value="Professional Qualification Expiration Date:" />
                                        <apex:outputField styleclass="top-level-label" value="{!licenseTransation.PQ_Expiration_Date__c}" />
                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel rendered="{! LicenseRecordTypeName != 'FCC_Assistant_Change_of_Address'}">
                                        <apex:outputLabel styleClass="top-level-label" value="Name of Family Child Care Educator you are working with?:" />
                                        <apex:outputField styleclass="top-level-label" value="{!licenseTransation.FCC_Educator_Assistant_is_working_with__c}" />
                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel rendered="{! LicenseRecordTypeName != 'FCC_Assistant_Change_of_Address'}">
                                        <apex:outputLabel styleClass="top-level-label" value="I am applying to be approved or certified as a:" />
                                        <apex:outputField styleclass="top-level-label" value="{!licenseTransation.License_Requested__c}" />
                                    </apex:outputpanel>
                                </div>
                            </div>
                        </div>
                    </apex:outputPanel>
