                                        <apex:outputLabel styleClass="top-level-label" value="Mailing Zip:" />
                                        <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Mailing_Zip__c}" />
                                    </apex:outputpanel>
                                </div>
                            </div>
                        </div>
