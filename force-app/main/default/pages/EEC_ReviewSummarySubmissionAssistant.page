                            <md-content class="transaction-info-row" layout="row">
                                <p class="md-subhead top-level-label">Reason for request: </p>
                                <apex:outputtext value="{!licenseTransation.Reason_for_Request__c}" />
                            </md-content>
                        </apex:outputpanel>
                        <apex:outputpanel rendered="{!LicenseRecordTypeName == 'Close_Prior_Expiration' || LicenseRecordTypeName == 'FCC_Inactivate'}">
                            <md-content class="transaction-info-row" layout="row">
