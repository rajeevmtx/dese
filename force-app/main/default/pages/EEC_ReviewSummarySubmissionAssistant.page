                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel >
                                        <apex:outputLabel styleClass="top-level-label" value="Last Name:" />
                                        <apex:outputField styleclass="top-level-label" value="{!transactionContact.Last_Name__c}" />
                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel >
                                        <apex:outputLabel styleClass="top-level-label" value="Date of Birth:" />
                                        <apex:outputField styleclass="top-level-label" value="{!transactionContact.Date_of_Birth__c}" />
                                    </apex:outputpanel>
                                </div>
                            </div>
                        </div>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!LicenseRecordTypeName!= 'Close_Prior_Expiration' 
                                                && LicenseRecordTypeName!= 'Re_issuance_of_License_Cert' && LicenseRecordTypeName!= 'FCC_Inactivate' 
                                                && LicenseRecordTypeName != 'FCC_Re_Active' && LicenseRecordTypeName !='FCC_Change_of_Name' 
                                                && LicenseRecordTypeName !='Background_Record_Check' && LicenseRecordTypeName != 'Variance_On_Regulation_Request'}">
                        <div class="panel panel-default ">
                            <div class="panel-heading">
                                <h3 class="panel-title">
                                    Contact Information
                                </h3>
                            </div>
                            <div class="panel-body">
                                <div>
                                    <apex:outputpanel id="phnnum">
                                        <apex:outputLabel styleClass="top-level-label" value="Phone Number:" />
                                        <apex:outputField styleclass="top-level-label"  />
                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel id="altphnum">
                                        <apex:outputLabel styleClass="top-level-label" value="Alternate Phone Number:" />
                                        <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider_Alternate_Phone__c}" />
                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel id="fax" rendered="{!LicenseRecordTypeName != 'FCC_Assistant_New_License'}">
                                        <apex:outputLabel styleClass="top-level-label" value="Fax Number:" />
                                        <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider_Fax_Number__c}" />
                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel id="email">
                                        <apex:outputLabel styleClass="top-level-label" value="Email address:" />
                                        <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider_Email__c}" />
                                    </apex:outputpanel>
                                </div>
                            </div>
                        </div>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!LicenseRecordTypeName!= 'Close_Prior_Expiration' && LicenseRecordTypeName!= 'Re_issuance_of_License_Cert' 
                                                    && LicenseRecordTypeName!= 'FCC_Inactivate' && LicenseRecordTypeName != 'FCC_Re_Active' 
                                                    && LicenseRecordTypeName !='FCC_Change_of_Name' 
                                                    && LicenseRecordTypeName != 'FCC_Assistant_Change_of_Address' 
                                                    && LicenseRecordTypeName !='Background_Record_Check' && LicenseRecordTypeName != 'Variance_On_Regulation_Request'	}">
