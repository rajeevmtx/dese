                                </apex:outputpanel>
                                <div>
                                    <apex:outputpanel id="street1">
                                        <apex:outputText styleClass="top-level-label" value="Street Address 1:" />
                                        <apex:outputText styleclass="top-level-label" value="{!licenseTransation.Provider__r.Provider_Address_1__c}" />
                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel id="street2">
