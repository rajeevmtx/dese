                                <apex:outputPanel rendered="{provider.PPM_Meeting_Attended_Date__c != null}">
                                    <label class="top-level-label">Date Attended Potential Provider Meeting :</label>
                                    <apex:outputField value="{!provider.PPM_Meeting_Attended_Date__c}"/>
                                </apex:outputPanel>
                            </div> -->
                                <apex:outputPanel rendered="{!provider.PPM_Meeting_Attended_Date__c == null && (LicenseRecordTypeName !='FCC_Change_of_Household' && LicenseRecordTypeName !='FCC_Change_Room_Floor_Level')}">
                                    <label class="top-level-label">Date Attended Potential Provider Meeting :</label>
                                    <apex:outputField value="{!licenseTransation.PPM_Attended_Date__c}" />
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!provider.PPM_Meeting_Attended_Date__c != null}">
                                    <label class="top-level-label">Date Attended Potential Provider Meeting :</label>
                                    <apex:outputField value="{!provider.PPM_Meeting_Attended_Date__c}" />
                                </apex:outputPanel>
                                <!--                      <div> -->
                                <!--                      <apex:outputpanel > -->
                                <!--                              <label class="top-level-label">Name of Family Child Care Educator you are working with? </label> -->
                                <!--                              <apex:outputText value="{!licenseTransation.FCC_Educator_Assistant_is_working_with__c}"/> -->
                                <!--                      </apex:outputpanel>  -->
                                <!--                      </div> -->
