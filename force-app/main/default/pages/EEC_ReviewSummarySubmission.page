                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel id="zipcode">
                                        <apex:outputText styleClass="top-level-label" value="Zip Code:" />
                                        <apex:outputtext styleclass="top-level-label" value="{!licenseTransation.Provider__r.Provider_Zip_Code__c}" />
                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel id="state">
                                        <apex:outputText styleClass="top-level-label" value="State:" />
                                        <apex:outputText styleclass="top-level-label" value="{!licenseTransation.Provider__r.State__c}" />
                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel >
                                        <apex:outputLabel styleClass="top-level-label" value="Do you live at this address?" rendered="{!LicenseRecordTypeName == 'FCC_New_License' || LicenseRecordTypeName == 'FCC_Renewal'}"
                                        />
                                        <apex:outputLabel styleClass="top-level-label" value="Do you live at new address?" rendered="{!LicenseRecordTypeName == 'Change_of_Address'}"
                                        />
                                        <apex:outputField styleclass="top-level-label" rendered="{!LicenseRecordTypeName == 'FCC_New_License'  ||  LicenseRecordTypeName == 'FCC_Renewal' || LicenseRecordTypeName == 'Change_of_Address'}"
                                            value="{!licenseTransation.Do_you_live_at_this_address__c}" />
                                    </apex:outputpanel>
                                </div>
                            </div>
                        </div>
                    </apex:outputPanel>
                    <apex:outputpanel rendered="{!LicenseRecordTypeName=='FCC_New_License'||LicenseRecordTypeName=='FCC_Renewal'||LicenseRecordTypeName=='Change_of_Address'}">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">
                                    Mailing Address</h3>
                            </div>
                            <div class="panel-body">
                                <apex:outputpanel >
                                    <apex:outputtext styleclass="top-level-label" value="Mailing Street:" />
                                    <apex:outputtext value="{!licenseTransation.Mailing_Street__c}" />
                                </apex:outputpanel>
                                <div>
                                    <apex:outputpanel >
                                        <apex:outputLabel styleClass="top-level-label" value="Mailing State:" />
                                        <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Mailing_State__c}" />
                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel >
                                        <apex:outputLabel styleClass="top-level-label" value="Mailing City:" />
                                        <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Mailing_City__c}" />
                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel >
                                        <apex:outputLabel styleClass="top-level-label" value="Mailing Zip:" />
                                        <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Mailing_Zip__c}" />
                                    </apex:outputpanel>
                                </div>
                            </div>
                        </div>
                    </apex:outputpanel>
                    <apex:outputPanel rendered="{!LicenseRecordTypeName != 'Close_Prior_Expiration'
                                                && LicenseRecordTypeName != 'Re_issuance_of_License_Cert' 
                                                && LicenseRecordTypeName != 'FCC_Renewal' 
                                                && LicenseRecordTypeName !='FCC_Change_of_Name' 
                                                && LicenseRecordTypeName != 'FCC_Inactivate' 
                                                && LicenseRecordTypeName != 'FCC_Re_Active' 
                                                && LicenseRecordTypeName != 'Variance_On_Regulation_Request'
                                                && LicenseRecordTypeName != 'FCC_Downgrade_Capacity'
                                                && LicenseRecordTypeName != 'FCC_Change_Room_Floor_Level'
                                                && LicenseRecordTypeName != 'Background_Record_Check' && LicenseRecordTypeName !='EEC_FCC_Modification'}">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">
                                    Provider Information</h3>
                            </div>
                            <div class="panel-body">
                                <apex:outputPanel rendered="{!LicenseRecordTypeName !='Variance_On_Regulation_Request'}">
                                    <div>
                                        <label class="top-level-label">First Name :</label>
                                        <apex:outputText value="{!transactionContact.First_Name__c}" />
                                    </div>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!LicenseRecordTypeName !='Variance_On_Regulation_Request'}">
                                    <div>
                                        <label class="top-level-label">Last Name : </label>
                                        <apex:outputText value="{!transactionContact.Last_Name__c}" />
                                    </div>
                                </apex:outputPanel>
                                <apex:outputpanel rendered="{!LicenseRecordTypeName=='FCC_New_License'}">
                                    <div>
                                        <label class="top-level-label">Date of Birth</label>
                                        <apex:outputField value="{!transactionContact.Date_of_Birth__c}" />
                                    </div>
                                </apex:outputpanel>
                            </div>
                        </div>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!LicenseRecordTypeName != 'Close_Prior_Expiration'
                                                && LicenseRecordTypeName != 'Re_issuance_of_License_Cert' 
                                                && LicenseRecordTypeName !='FCC_Change_of_Name' 
                                                && LicenseRecordTypeName != 'FCC_Inactivate' 
                                                && LicenseRecordTypeName != 'FCC_Re_Active' 
                                                && LicenseRecordTypeName != 'Variance_On_Regulation_Request'
                                                && LicenseRecordTypeName != 'FCC_Downgrade_Capacity'
                                                && LicenseRecordTypeName != 'Background_Record_Check'}">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">
                                    Contact Information
                                </h3>
                            </div>
                            <div class="panel-body">
                                <div>
                                    <apex:outputpanel id="phnnum">
                                        <apex:outputLabel styleClass="top-level-label" value="Phone Number:" />
                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputpanel id="altphnum">
                                        <apex:outputLabel styleClass="top-level-label" value="Alternate Phone Number:" />
                                        <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider_Alternate_Phone__c}" />
                                    </apex:outputpanel>
                                </div>
                                <!--              <div> -->
