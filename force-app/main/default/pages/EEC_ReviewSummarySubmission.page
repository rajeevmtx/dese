                                <!--                <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider__r.Fax}" /> -->
                                <!--              </apex:outputpanel> -->
                                <!--              </div> -->
                                <div>
                                    <apex:outputpanel id="email">
                                        <apex:outputLabel styleClass="top-level-label" value="Email address:" />
                                        <apex:outputField styleclass="top-level-label" value="{!licenseTransation.Provider_Email__c}" />
                                    </apex:outputpanel>
                                </div>
                            </div>
                        </div>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!LicenseRecordTypeName != 'Close_Prior_Expiration'
                                                && LicenseRecordTypeName != 'Re_issuance_of_License_Cert' 
                                                && LicenseRecordTypeName !='FCC_Change_of_Name' 
                                                && LicenseRecordTypeName != 'FCC_Inactivate' 
                                                && LicenseRecordTypeName != 'FCC_Re_Active' 
                                                && LicenseRecordTypeName != 'Variance_On_Regulation_Request'
                                                && LicenseRecordTypeName != 'FCC_Downgrade_Capacity'
                                                && LicenseRecordTypeName != 'Background_Record_Check' && LicenseRecordTypeName !='EEC_FCC_Modification'}">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">
                                    Other Information</h3>
                            </div>
                            <div class="panel-body">
                                <div>
                                    <apex:outputPanel >
                                        <label class="top-level-label">Preferred Communication Language :</label>
                                        <apex:outputField value="{!licenseTransation.Preferred_Language__c}" />
                                    </apex:outputPanel>
                                </div>
                                <div>
                                    <apex:outputpanel rendered="{!LicenseRecordTypeName=='FCC_Renewal' || LicenseRecordTypeName=='FCC_New_License' || LicenseRecordTypeName=='FCC_License_Upgrade' || isFccAssistant}">
                                        <label class="top-level-label">Professional Qualification Registry Number :</label>
                                        <apex:outputfield value="{!licenseTransation.Prof_Qualification_Reg_Number__c}" />
                                    </apex:outputpanel>
                                </div>
                                <div>
                                    <apex:outputPanel >
                                        <label class="top-level-label">Can you communicate in English? </label>
                                        <apex:selectRadio disabled="true" value="{!licenseTransation.Can_Communicate_in_English__c}">
                                            <apex:selectOptions value="{!items}" />
                                        </apex:selectRadio>
                                    </apex:outputPanel>
                                </div>
                                <div>
                                    <apex:outputpanel rendered="{!LicenseRecordTypeName=='FCC_Renewal' || LicenseRecordTypeName=='FCC_New_License' || LicenseRecordTypeName=='FCC_License_Upgrade' || isFccAssistant}">
                                        <label class="top-level-label">Professional Qualification Expiration Date :</label>
                                        <apex:outputfield value="{!licenseTransation.PQ_Expiration_Date__c}" />
                                    </apex:outputpanel>
                                </div>
