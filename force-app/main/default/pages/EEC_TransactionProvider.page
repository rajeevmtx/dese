                                <apex:outputText rendered="{!LicenseRecordTypeName == 'GSA_New_License' ||LicenseRecordTypeName == 'GSA_Renewal' || LicenseRecordTypeName == 'GSA_Move' || LicenseRecordTypeName == 'GSA_Temporary_Move' || LicenseRecordTypeName == 'GSA_Change_of_Admin_Authority' || LicenseRecordTypeName == 'GSA_License_Change' || LicenseRecordTypeName == 'Large_Group'}">
