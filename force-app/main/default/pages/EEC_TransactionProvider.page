                                                            <apex:selectRadio value="{!licenseTransation.Mixed_Type_Program__c}" style="margin-bottom: 10px;">
                                                                <apex:selectoptions value="{!items}" html-data-parsley-required="true" />
                                                                <apex:actionSupport event="onchange" action="{!setLicensedAgency}" reRender="prgDesc" status="status"/>
                                                            </apex:selectRadio>
                                                        </div>
                                                    </apex:actionRegion>
                                                </fieldset>
                                            </apex:outputPanel>
                                        </div>
