                                            <apex:outputPanel rendered="{!providerRecordType=='Temporary Shelter'}">
                                                <label for="contracted" class="input-label">Are you also applying for the ability to conduct Group Care programming?
                                                    <span class="required">*</span>
                                                </label>
                                            </apex:outputPanel>
