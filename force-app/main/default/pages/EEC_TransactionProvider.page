    docType="html-5.0">
    <title>EEC : Transaction - Transaction/Provider Information</title>
    <c:EEC_Header id="pageHeader" />
    <script>
        j$(document).ready(function () {
            j$('.phonenumber').inputmask({ "mask": "(999) 999-9999" }); //specifying options
            j$('.Mailing_Zip').inputmask({
                "mask": "99999"
            });
            if ('{!LicenseRecordTypeName}' == 'FCC_Assistant_New_License') {
                updateLicenseRequestedPicklist();
            }
            j$("[id*=mailingState]").prop("disabled", true);
            if ('{!LicenseRecordTypeName}' == 'Variance_On_Regulation_Request') {
                j$('input').attr('readonly', 'true');
                j$('select').attr('readonly', 'true');
            }
            j$(".navigation-buttons a").click(function () {
            });
            var isSubmitted = '{!isSubmitted}';
            if (isSubmitted == 'true') {
                j$('.saveButton').addClass('visibilityHidden');
                j$("input[type=text]").prop('disabled', true);
                j$("input[type=radio]").prop('disabled', true);
                j$("select").prop('disabled', true);
            }
            updateDatalist();
            renderUmbrellaPanel();
            isNonEnglishLanguage();
        });
        function isNonEnglishLanguage() {
            var language = j$('.language').val();
            if (language == 'Other') {
                j$('.nonEnglish').show();
            } else {
                j$('.nonEnglish').hide();
            }
        }
        function renderUmbrellaPanel() {
            var ownerShip = j$('.ownerShip').val();
            if (ownerShip != null && ownerShip != '' && ownerShip != undefined) {
                if (ownerShip == 'Non-Profit Corp.' || ownerShip == 'For Profit Corp.' || ownerShip == 'LLC') {
                    j$('.ownerShipPanel').removeClass('hidePanel');
                }
            }
        }
        function updateLicenseRequestedPicklist() {
            if ('{!LicenseRecordTypeName}' == 'FCC_Assistant_New_License') {
                j$("[id*=requestedLicensePicklistHidden]").val(j$("[id*=requestedLicensePicklist] :selected").text());
            }
        }
        function changeState(state) {
            var regExp = /\(([^)]+)\)/;
            var st = regExp.exec(j$("select option[value = '" + state.value + "']").text());
            j$("[id*=mailingState]").val(st[1]);
        }
        function changeProviderState(state) {
            var regExp = /\(([^)]+)\)/;
            var st = regExp.exec(j$("select option[value = '" + state.value + "']").text());
            j$("[id*=providerState]").val(st[1]);
        }
        function updateDatalist() {
            j$("datalist > option").each(function () {
                var origanalText = this.text;
                this.value = origanalText.substring(0, origanalText.indexOf('-'));
                this.text = origanalText.substring(origanalText.indexOf('-') + 1, origanalText.length);
            });
        }
        function phoneMask(obj) {
            if (!jQuery.isNumeric(obj.value)) {
                obj.value = "";
                return;
            } else {
                if (obj.value < 0) {
                    obj.value = "";
                    return;
                }
            }
            var x = obj.value.replace(/\D/g, '').match(/(\d{3})(\d{3})(\d{4})/);
            obj.value = '(' + x[1] + ') ' + x[2] + '-' + x[3];
        }
        function isNumber(evt) {
            evt = (evt) ? evt : window.event;
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                return false;
            }
            return true;
        }
        function isNumberBlur(obj) {
            if (!jQuery.isNumeric(obj.value)) {
                obj.value = "";
            } else {
                if (obj.value < 0) {
                    obj.value = "";
                }
            }
        }
        function onlyAlphabets(e, t) {
            try {
                if (window.event) {
                    var charCode = window.event.keyCode;
                } else if (e) {
                    var charCode = e.which;
                } else { return true; }
                if ((charCode > 64 && charCode < 91) || (charCode > 96 && charCode < 123))
                    return true;
                else
                    return false;
            } catch (err) {
                alert(err.Description);
            }
        }
        function onlyAlphabetsBlur(obj) {
            var val = obj.value;
            for (i = 0; i < val.length; i++) {
                var code = val.charCodeAt(i);
                if (code <= 64 || (code >= 91 && code <= 96) || code >= 123) {
                    obj.value = "";
                    return;
                }
            }
        }
    </script>
