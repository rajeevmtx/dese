                                                           && LicenseRecordTypeName!='GSA_Move'  
                                                           && LicenseRecordTypeName!='GSA_Temporary_Move'  
                                                           && LicenseRecordTypeName!='GSA_Change_of_Admin_Authority'
                                                           && LicenseRecordTypeName!='EEC_FCC_Modification'
